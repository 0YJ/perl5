	|	LVALFUN sexpr	%prec '('
			{ $$ = redOP($1, 1, lv($2)); }
	|	LVALFUN
			{ $$ = redOP($1, 1,
			    lv(gv_to_op(A_STAB,defstab))); }
	|	SSELECT
			{ $$ = redOP(OP_SELECT, 0);}
	|	SSELECT  WORD
			{ $$ = redOP(OP_SELECT, 1,
			    gv_to_op(A_WORD,newGV($2,TRUE)));
			    Safefree($2); $2 = Nullch; }
	|	SSELECT '(' handle ')'
			{ $$ = redOP(OP_SELECT, 1, $3); }
	|	SSELECT '(' sexpr csexpr csexpr csexpr ')'
			{ op4 = $6;
			  $$ = redOP(OP_SSELECT, 4, $3, $4, $5); }
